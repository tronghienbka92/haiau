                                </td>
                            </tr>
                        </table>
                        <script>
                            $(document).ready(function () {
                                $('#cbbSearchNguoiGui').kendoComboBox({
                                    placeholder: "Điện thoại người gửi",
                                    dataTextField: "SoDienThoai",
                                    dataValueField: "Id",
                                    filter: "contains",
                                    autoBind: false,
                                    minLength: 5,
                                    dataSource: {
                                        type: "json",
